<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name ="Amazon Test Script" parallel="false">

        <parameter name="useCloudEnv" value="true"></parameter>
        <parameter name="cloudEnvName" value="browserstack"></parameter>
        <parameter name="os" value="Windows"></parameter>
        <parameter name="os_version" value="10"></parameter>
        <parameter name="browserName" value="Chrome"></parameter>
        <parameter name="browserVersion" value="84.0"></parameter>
        <parameter name="url" value="http://amazon.com"></parameter>

    <test name = "Amazon Home">

        <classes>
                        <class name="search.SearchItems"/>
                        <class name="search.DropDown"></class>
        </classes>

    </test>

</suite>